{
    "name": "aarsteinmedia/flatsome-stubs",
    "description": "Flatsome function, class, and global variable declaration stubs for easier static analysis.",
    "homepage": "https://github.com/aarsteinmedia/flatsome-stubs",
    "keywords": [
        "flatsome",
        "static analysis"
    ],
    "require-dev": {
        "php": ">=7.1",
        "ext-gettext": "*",
        "squizlabs/php_codesniffer": "*",
        "wp-coding-standards/wpcs": "*",
        "dealerdirect/phpcodesniffer-composer-installer": "*",
        "php-stubs/generator": "^0.5.0"
    },
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Johan Martin Aarstein",
            "email": "johan@aarstein.media"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
